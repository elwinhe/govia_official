<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/logo7"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.306"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/govia_medical_logo"
        />

    <ImageView
        android:id="@+id/logo8"
        android:layout_width="143dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="@+id/logo7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/logo7"
        app:layout_constraintTop_toTopOf="@+id/logo7"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/medical_information_text"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="167dp"
        android:layout_height="23dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="20dp"
        android:text="Add Insurance Card"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo7" />

    <EditText
        android:id="@+id/memberName"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:text="Member Name"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/MemberID"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:text="Member ID"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/memberName"
        app:layout_constraintTop_toBottomOf="@+id/memberName" />

    <EditText
        android:id="@+id/planProvider"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:text="Plan Provider"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/groupNumber2"
        app:layout_constraintTop_toBottomOf="@+id/groupNumber2" />

    <EditText
        android:id="@+id/planType"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:text="Plan Type"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/planProvider"
        app:layout_constraintTop_toBottomOf="@+id/planProvider" />

    <EditText
        android:id="@+id/copay"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Copay"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/planType"
        app:layout_constraintTop_toBottomOf="@+id/planType" />

    <EditText
        android:id="@+id/rxBin"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:text="Rx Bin"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/planType"
        app:layout_constraintTop_toBottomOf="@+id/copay" />

    <EditText
        android:id="@+id/employer"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:text="Employer"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/rxBin"
        app:layout_constraintTop_toBottomOf="@+id/rxBin" />

    <EditText
        android:id="@+id/groupNumber2"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        android:singleLine="true"
        android:text="Group Number"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/MemberID"
        app:layout_constraintTop_toBottomOf="@+id/MemberID" />

    <Button
        android:id="@+id/backButton2"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="48dp"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <Button
        android:id="@+id/nextButton3"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="48dp"
        android:text="Next"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>